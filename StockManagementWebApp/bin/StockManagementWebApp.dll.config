<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <connectionStrings>
    <add name="StockDB" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=StockManagementDB;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.webServer>
<directoryBrowse enabled="false" />
<defaultDocument enabled="true">
      <files>
        <clear/>
        <add value="UI/IndexUI.aspx"/>
      </files>
    </defaultDocument>
</system.webServer>

</configuration>